                            <img src="~/Content/images/logo.jpg" alt="Image" class="img-fluid">
                        </a>
                    </div>
                    <div class="mr-auto">

                        <nav class="site-navigation position-relative text-right" role="navigation">

                            <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">

                                <li class="active">
                                    @Html.ActionLink("Home", "Index", "Home", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li class="has-children">
                                    <a href="about.html" class="nav-link text-left">About Us</a>
                                    <ul class="dropdown">
                                        <li><a href="teachers.html">Our Teachers</a></li>
                                        <li><a href="about.html">Our School</a></li>
                                    </ul>
                                </li>
                                <li>
                                    @Html.ActionLink("Admissions", "Index", "Admissions", new { @class = "nav-link text-left" }, null)
                                </li>
                                <li>
                                    @Html.ActionLink("Courses", "Index", "Courses", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li>
                                    @Html.ActionLink("Contact", "Index", "Contact", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li>@Html.ActionLink("Admin", "Admin", "Admin", new { area = "Admin" }, new { })</li>
                            </ul>                                                                                                                                                                                                                                                                                          </ul>
                        </nav>
                      
                    </div>


                </div>
            </div>





